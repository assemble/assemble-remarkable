{
  "name": "assemble-remarkable",
  "description": "Assemble pipeline plugin for remarkable, the markdown converter for node.js. Can also be used with gulp.",
  "version": "1.1.1",
  "homepage": "https://github.com/assemble/assemble-remarkable",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "assemble/assemble-remarkable",
  "bugs": {
    "url": "https://github.com/assemble/assemble-remarkable/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "middleware.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "highlight.js": "^9.12.0",
    "plugin-error": "^0.1.2",
    "remarkable": "^1.7.1",
    "through2": "^2.0.3",
    "unescape": "^1.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2",
    "vinyl": "^2.0.2"
  },
  "keywords": [
    "assemble",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "plugin",
    "project",
    "projects",
    "remarkable",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "yeoman",
    "yo"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "related": {
      "highlight": "gulp-breakdance",
      "list": [
        "breakdance",
        "gulp-html-toc",
        "gulp-htmlmin",
        "gulp-remarkable",
        "remarkable"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "assemble",
      "highlight.js",
      "remarkable"
    ]
  }
}
